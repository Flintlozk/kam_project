      "@plusmar-front/pipeline-service": ["apps/plusmar-front-end/src/app/services/facebook/pipeline/pipeline.service"],
      "@plusmar-front/shared-service": ["apps/plusmar-front-end/src/app/services/facebook/shared/shared.service"],
      "@plusmar-front/thread-service": ["apps/plusmar-front-end/src/app/services/facebook/thread/thread.service.ts"],
