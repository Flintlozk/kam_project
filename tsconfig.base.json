      "@reactor-room/cms-cdk": ["libs/cms-cdk/src/index.ts"],
      "@reactor-room/cms-environment-services": ["libs/cms-environment-services/src/index.ts"],
      "@reactor-room/cms-frontend-helpers-lib": ["libs/cms-frontend-helpers-lib/src/index.ts"],
      "@reactor-room/cms-frontend-services-lib": ["libs/cms-frontend-services-lib/src/index.ts"],
      "@reactor-room/cms-model-mongo-lib": ["libs/cms-model-mongo-lib/src/index.ts"],
      "@reactor-room/cms-models-lib": ["libs/cms-models-lib/src/index.ts"],
      "@reactor-room/cms-services-lib": ["libs/cms-services-lib/src/index.ts"],
      "@reactor-room/crm-models-lib": ["libs/crm-models-lib/src/index.ts"],
